class Solution:
    def maxProfit(self, prices) -> int:

        maxprofit = 0

        for x in range(len(prices)):
            if len(prices[x+1:]) == 0:
                continue
            if max(prices[x+1:]) - prices[x] > maxprofit:
                maxprofit = max(prices[x+1:]) - prices[x]
        return maxprofit
