class Solution:
    def maxProfit(self, prices) -> int:

        maxprofit = 0

        for x in range(len(prices)):

            if len(prices[x+1:]) == 0:
                continue
            if prices[x] > max(prices[x+1:]):
                continue
            total = max(prices[x+1:]) - prices[x]
            if maxprofit < total:
                maxprofit = total

        return maxprofit
