class Solution:
    def maxProfit(self, prices) -> int:

        maxprofit = 0
        start = 0


        while start != len(prices) -1:

            value = prices[start]

            if max(prices[start+1:]) < value:
                start += 1
                continue

            total = max(prices[start+1:]) - prices[start]

            if maxprofit < total:
                maxprofit = total
            start += 1

        return maxprofit
