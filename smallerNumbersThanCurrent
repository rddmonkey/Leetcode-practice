class Solution:
    def smallerNumbersThanCurrent(self, nums):

        ordered = sorted(nums)
        mydict = {}
        ans = []

        for i in range(len(ordered)):
            if ordered[i] not in mydict:
                mydict[ordered[i]] = i

        for i in range(len(nums)):
            ans.append(mydict[nums[i]])

        return ans
