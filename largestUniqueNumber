class Solution:
    def largestUniqueNumber(self, nums) -> int:

        mydict = {}
        high = float("-inf")


        for x in nums:
            if x not in mydict:
                mydict[x] = 1
            else:
                mydict[x] +=1

        for v,k in mydict.items():
            if k == 1:
                if v > high:
                    high = v

        if high == float("-inf"):
            return -1

        return high
