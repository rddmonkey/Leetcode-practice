class Solution:
    def findErrorNums(self, nums):

        ans = []

        mydict = {}

        for x in nums:
            if x not in mydict:
                mydict[x] = 1
            else:
                mydict[x] += 1

        for k,v in mydict.items():
            if v == 2:
                ans.append(k)

        for i in range(1,len(nums)+1):
            if i not in mydict.keys():
                ans.append(i)
        return ans
