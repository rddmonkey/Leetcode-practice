#Origin https://leetcode.com/problems/longest-common-prefix/

#took 2 hours, thought it was easy at first. Not liking the list I made or the try/except. Will likely refactor later.

class Solution:
    def longestCommonPrefix(self, strs) -> str:
        flag = True
        ans = ""
        start = 0
        test = []


        while flag == True:
            for x in range(len(strs)):
                try:
                    test.append(strs[x][start])
                    if x == len(strs)-1:
                        if len(set(test)) == 1:
                            ans += test[0]
                            test=[]
                            start+=1
                            break
                        else:
                            flag = False
                    if strs[x][start] in set(test):
                        continue
                except:
                    return ans

        return ans
