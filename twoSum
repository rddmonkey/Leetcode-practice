class Solution:
    def twoSum(self, numbers, target):

        ans = []
        startfront = 0
        startback = -1

        while ans == []:
            if numbers[startfront] + numbers[startback] == target:
                return [startfront+1,startback+len(numbers)+1]
            elif numbers[startfront] + numbers[startback] > target:
                startback -= 1
            else:
                startfront +=1
