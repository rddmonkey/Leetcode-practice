class Solution:
    def isArmstrong(self, n: int) -> bool:

        ans = n
        nums = []

        while n > 10:
            nums.append(n%10)
            n = n//10

        nums.append(n)

        for x in range(len(nums)):
            nums[x] = nums[x]**len(nums)

        return ans == sum(nums)
